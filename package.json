{
	"name": "cpp-class-creator",
	"publisher": "birgersp",
	"repository": {
		"url": "https://github.com/birgersp/vscode-cpp-class-creator.git"
	},
	"displayName": "C++ Class Creator",
	"description": "Quick C++ class creation from context menu or hotkey",
	"version": "1.0.6",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cpp-class-creator.createClass"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cpp-class-creator.createClass",
				"title": "Create C++ class"
			}
		],
		"keybindings": [
			{
				"command": "cpp-class-creator.createClass",
				"key": "alt+x"
			}
		],
		"menus": {
			"explorer/context": [{
				"command": "cpp-class-creator.createClass",
				"group": "cppcreator@1"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
